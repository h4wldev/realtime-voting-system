{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "ordered-imports": false,
    "no-bitwise": false,
    "no-reference": false,
    "object-literal-sort-keys": false,
    "no-consecutive-blank-lines": [true, 2],
    "max-classes-per-file": [false],
    "no-var-requires": false
  },
  "rulesDirectory": []
}
